- if !defined?(mission_file)
  - mission_file = @mission_file
.row.mission_file_row
  .col-md-12 
    %img{:src=>mission_file[:icon_link]}
    =link_to mission_file[:alternate_link], :target=>"_blank", class: 'file-link' do
      %strong= mission_file[:title]
    .pull-right.platform_timestamp{"data-time-stamp"=>mission_file[:modified_date]}=format_gdrive_timestamp(mission_file[:modified_date])
    / -if !current_user.nil? && (current_user.id == mission_file.user_id || @current_member_role.is_coordinator_or_greater?)
    /   = link_to 'Unshare', mission_file, :method => :delete, :data => { :confirm => 'Are you sure?' }, :class=>""
